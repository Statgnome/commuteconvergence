#with these 5 lines, which could arguably be one or two lines
sig <- tukeydf %>%
rownames_to_column('group') %>%
filter(p.adj < .05) %>%
column_to_rownames('group') %>%
rownames()
paste("Your commutes are significantly different between these time clusters:",sep="") %>%
print()
